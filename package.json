{
  "name": "speed",
  "version": "1.0.0",
  "description": "My supreme Nuxt.js project",
  "author": "Трофимов Илья",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/server/index.js --watch src/api",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node ./src/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "tslint": "tslint -p tsconfig.json \"./src/**/*.{d.ts,ts,tsx}\"",
    "tslint:fix": "tslint -p tsconfig.json --fix \"./src/**/*.{d.ts,ts,tsx}\"",
    "prettier:base": "prettier --config ./.prettierrc",
    "prettier:check": "npm run prettier:base -- --list-different \"./src/**/*.{d.ts,ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*.{vue,d.ts,ts,tsx}\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "codemirror": "^5.49.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-session": "^1.15.3",
    "md5": "^2.2.1",
    "mongoose": "^5.2.6",
    "morgan": "^1.9.0",
    "nuxt": "^2.9.2",
    "nuxt-property-decorator": "^1.2.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "vue-class-component": "^6.3.2",
    "vue-codemirror": "^4.0.6",
    "vuep": "^0.8.1",
    "vuex-class": "^0.3.1"
